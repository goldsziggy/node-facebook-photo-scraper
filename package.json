{
  "name": "node-facebook-photo-scraper",
  "version": "1.0.0",
  "description": "A project designed to utilize a Facebook User Access Token to scrape all of Facebook's metedata on your photos and store locally, thus allowing you to serve up your Facebook photos",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node",
    "build": "babel src -d dist",
    "serve": "node dist/index.js",
    "test": "mocha --compilers js:babel-register"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0",
    "fb": "^1.1.1",
    "mongoose": "^4.5.8",
    "nodemon": "^1.10.0",
    "shuffle-array": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.11.6",
    "mocha": "^3.0.2"
  }
}
